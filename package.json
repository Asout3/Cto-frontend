{
  "name": "cto-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0 --port ${PORT:-3000}",
    "build": "next build",
    "start": "next start --hostname 0.0.0.0 --port ${PORT:-3000}",
    "lint": "next lint",
    "lint:fix": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint . --max-warnings=0 --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "prepare": "simple-git-hooks",
    "check": "npm run lint && npm run typecheck"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.3",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@supabase/supabase-js": "^2.46.1",
    "axios": "^1.7.7",
    "better-auth": "^1.4.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^11.2.12",
    "lucide-react": "^0.445.0",
    "next": "^15.5.7",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.5.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "cross-env": "^10.1.0",
    "eslint": "^9.11.1",
    "eslint-config-next": "^15.1.6",
    "eslint-config-prettier": "^9.1.0",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "cross-env ESLINT_USE_FLAT_CONFIG=false eslint --fix",
      "prettier --write"
    ],
    "*.{mjs,cjs,json,md,css,scss}": "prettier --write"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
